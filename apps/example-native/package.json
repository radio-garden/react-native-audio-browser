{
  "name": "example-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:adb-reverse": "adb reverse tcp:8081 tcp:8081 && adb forward tcp:5277 tcp:5277",
    "android:adb-stop": "adb shell am force-stop com.audiobrowserexample",
    "android:build": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "android:clean": "rm -rf android/.gradle && rm -rf android/build && rm -rf android/app/build && rm -rf android/app/.cxx",
    "android:dhu": "pkill -f 'desktop-head-unit' || true && $HOME/Library/Android/sdk/extras/google/auto/desktop-head-unit",
    "android:format": "cd android && ./gradlew :react-native-audio-browser:format",
    "android:ide": "open -a /Applications/Android\\ Studio.app android",
    "android:install-release": "RD=android/app/build/outputs/bundle/release && (rm $RD/app.apks || true) && bundletool build-apks --bundle=$RD/app-release.aab --output=$RD/app.apks && (adb uninstall com.example || true) && bundletool install-apks --apks=$RD/app.apks",
    "android:logs": "react-native log-android",
    "android:release": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd app/build/outputs/bundle/release && pwd && ls",
    "android:test": "cd android && ./gradlew :react-native-audio-browser:testDebugUnitTest",
    "android:uninstall": "adb uninstall com.audiobrowserexample",
    "android:search": "adb shell \"am start-foreground-service -a android.media.action.MEDIA_PLAY_FROM_SEARCH -n com.audiobrowserexample/com.audiobrowser.Service --es query '$@'\"",
    "android": "react-native run-android",
    "format": "prettier --write src App.tsx",
    "ios:build": "react-native build-ios --mode Debug",
    "ios:ide": "open ios/*.xcworkspace",
    "ios:sim": "react-native run-ios --simulator=\"iPhone\"",
    "ios": "react-native run-ios --simulator='iPhone 16'",
    "lint": "eslint . --max-warnings=0 --fix",
    "pod": "bundle install && bundle exec pod install --project-directory=ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "types": "tsc --noEmit true"
  },
  "dependencies": {
    "@react-native-vector-icons/fontawesome6": "^12.0.1",
    "@react-native/new-app-screen": "0.82.0",
    "common-app": "*",
    "react": "19.1.1",
    "react-native": "0.82.0",
    "react-native-audio-browser": "*",
    "react-native-nitro-modules": "0.31.4",
    "react-native-safe-area-context": "^5.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.0",
    "@react-native/eslint-config": "0.82.0",
    "@react-native/metro-config": "0.82.0",
    "@react-native/typescript-config": "0.82.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.39.1",
    "prettier": "^3.0.3",
    "react-native-monorepo-config": "^0.3.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=24"
  }
}
