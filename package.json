{
  "name": "react-native-audio-browser",
  "version": "0.1.0",
  "type": "module",
  "description": "react-native-audio-browser is a react native package built with Nitro",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "scripts": {
    "android:format": "yarn workspace example-native android:format",
    "android:ide": "yarn workspace example-native android:ide",
    "android:logs": "react-native log-android",
    "android:test": "yarn workspace example-native android:test",
    "build": "yarn prepare",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "codegen": "node codegen/index.js --logLevel=\"debug\" && node post-script.cjs && yarn build",
    "prepare": "bob build",
    "example": "yarn workspace example-native",
    "ci:format": "prettier --check src",
    "ci:lint": "eslint src --max-warnings=0",
    "format": "prettier --write src",
    "ios:format": "swiftformat ios",
    "ios:ide": "yarn workspace example-native ios:ide",
    "ios:rebuild": "yarn workspace example-native ios:rebuild",
    "lint": "eslint . --fix",
    "release": "semantic-release",
    "test": "jest",
    "types": "tsc --noEmit"
  },
  "keywords": [
    "react-native",
    "react-native-audio-browser"
  ],
  "files": [
    "src",
    "react-native.config.js",
    "lib",
    "nitrogen",
    "cpp",
    "nitro.json",
    "android/build.gradle",
    "android/fix-prefab.gradle",
    "android/gradle.properties",
    "android/CMakeLists.txt",
    "android/src",
    "ios/**/*.h",
    "ios/**/*.m",
    "ios/**/*.mm",
    "ios/**/*.cpp",
    "ios/**/*.swift",
    "app.plugin.js",
    "*.podspec",
    "README.md"
  ],
  "workspaces": [
    "apps/*"
  ],
  "repository": "https://github.com/puckey/react-native-audio-browser.git",
  "author": "puckey",
  "license": "MIT",
  "bugs": "https://github.com/puckey/react-native-audio-browser/issues",
  "homepage": "https://github.com/puckey/react-native-audio-browser#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@jamesacarr/eslint-formatter-github-actions": "^0.2.0",
    "@react-native-community/cli": "15.0.0-alpha.2",
    "@react-native/eslint-config": "^0.78.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.12",
    "@types/react": "19.1.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "del-cli": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "nitrogen": "^0.31.10",
    "prettier": "^3.0.3",
    "react": "19.1.1",
    "react-native": "0.83.0",
    "react-native-builder-bob": "^0.37.0",
    "react-native-monorepo-config": "^0.3.1",
    "react-native-nitro-modules": "0.31.10",
    "semantic-release": "^24.2.9",
    "shaka-player": "^4.15.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.46.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-nitro-modules": "*",
    "shaka-player": "^4.7.9"
  },
  "peerDependenciesMeta": {
    "shaka-player": {
      "optional": true
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.json"
        }
      ]
    ]
  },
  "packageManager": "yarn@4.12.0"
}
